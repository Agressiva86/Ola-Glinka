---
import Layout from "../layouts/Layout.astro";

import Header from "../components/Header.astro";
import Welcome from "../components/Welcome.astro";
import Omnie from "../components/Omnie.astro";
import OmnieBox from "../components/OmnieBox.astro";
import Opinie from "../components/Opinie.astro";
import Help from "../components/Help.astro";
import Gallery from "../components/Gallery.astro";
import Public from "../components/Public.astro";
import Cooperation from "../components/Cooperation.astro";
import Footer from "../components/Footer.astro";
import Contact from "../components/Contact.astro";
import OpinieMobile from "../components/OpinieMobile.astro";


// Welcome to Astro! Wondering what to do next? Check out the Astro documentation at https://docs.astro.build
// Don't want to use any of this? Delete everything in this file, the `assets`, `components`, and `layouts` directories, and start fresh.
---

<Layout>
	<Header />
	<div class="container max-w-[1550px] mx-auto px-10 grid grid-cols-1 gap-20 lg:gap-24 py-20 lg:py-24">
		<Welcome />
		<Omnie />
	</div>

	<OmnieBox />

	<div class="container max-w-[1550px] mx-auto px-10 grid grid-cols-1 gap-20 lg:gap-24 py-20 lg:py-24">
		<Opinie />
	</div>

	<OpinieMobile />

	<Help />

	<div class="container max-w-[1550px] mx-auto px-10 grid grid-cols-1 gap-20 lg:gap-24 py-20 lg:py-24">
		<Gallery />
		<Public />
		<Cooperation />
	</div>

	<Contact />

	<div class="container max-w-[1550px] mx-auto px-10 grid grid-cols-1 gap-20 lg:gap-24">
		<Footer />
	</div>
	
</Layout>
